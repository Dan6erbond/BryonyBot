{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended"
  ],
  "jsRules": {},
  "rules": {
    "no-console": false,
    "align": [
      true,
      "statements",
      "members",
      "elements"
    ],
    "max-line-length": [
      true,
      120
    ],
    "no-any": false,
    "no-submodule-imports": false,
    "no-implicit-dependencies": [
      true,
      "dev",
      [
        "@",
        "@public",
        "@main",
        "@models",
        "@renderer",
        "@utils"
      ]
    ],
    "no-import-side-effect": false,
    "no-increment-decrement": [
      "allow-for-loops"
    ]
  }
}
